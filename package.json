{
  "name": "react-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "heroku-postbuild": "npm install --prefix ./Client && npm run build --prefix ./Client",
    "start": "node server/app.js",
    "client": "npm start --prefix ./Client",
    "server": "nodemon server/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "engines": {
    "node": "14.15.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "concurrently": "^6.0.2",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.7"
  }
}
